                        "properties": {
                            "arch": {
                                {% if synthetic_source | string | lower == "false" %}"ignore_above": 1024,{% endif %}
                                "type": "keyword"
                            },
                            "max_procs": {
                                "type": "long"
                            },
                            "os": {
                                {% if synthetic_source | string | lower == "false" %}"ignore_above": 1024,{% endif %}

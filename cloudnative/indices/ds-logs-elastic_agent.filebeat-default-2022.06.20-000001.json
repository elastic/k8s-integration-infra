{
    "mappings": {
        "_data_stream_timestamp": {
            "enabled": true
        },
        "date_detection": false,
        "_source": {
            "enabled": {{ source_enabled | default(false) | tojson }}{% if synthetic_source | default(False) | string | lower == "true" %},
            "synthetic": true
            {% endif %}
        },
        "dynamic_templates": [
            {
                "match_ip": {
                    "mapping": {
                        "type": "ip"
                    },
                    "match": "ip",
                    "match_mapping_type": "string"
                }
            },
            {
                "match_message": {
                    "mapping": {
                        "type": "match_only_text"
                    },
                    "match": "message",
                    "match_mapping_type": "string"
                }
            },
            {
                "strings_as_keyword": {
                    "mapping": {
                        {% if synthetic_source | string | lower == "false" %}"ignore_above": 1024,{% endif %}
                        "type": "keyword"
                    },
                    "match_mapping_type": "string"
                }
            }
        ],
        "properties": {
            "@timestamp": {
                "type": "date"
            },
            "agent": {
                "properties": {
                    "ephemeral_id": {
                        {% if synthetic_source | string | lower == "false" %}"ignore_above": 1024,{% endif %}
                        "type": "keyword"
                    },
                    "id": {
                        {% if synthetic_source | string | lower == "false" %}"ignore_above": 1024,{% endif %}
                        "type": "keyword"
                    },
                    "name": {
                        {% if synthetic_source | string | lower == "false" %}"ignore_above": 1024,{% endif %}
                        "type": "keyword"
                    },
                    "type": {
                        {% if synthetic_source | string | lower == "false" %}"ignore_above": 1024,{% endif %}
                        "type": "keyword"
                    },
                    "version": {
                        {% if synthetic_source | string | lower == "false" %}"ignore_above": 1024,{% endif %}
                        "type": "keyword"
                    }
                }
            },
            "cloud": {
                "properties": {
                    "account": {
                        "properties": {
                            "id": {
                                {% if synthetic_source | string | lower == "false" %}"ignore_above": 1024,{% endif %}
                                "type": "keyword"
                            }
                        }
                    },
                    "availability_zone": {
                        {% if synthetic_source | string | lower == "false" %}"ignore_above": 1024,{% endif %}
                        "type": "keyword"
                    },
                    "instance": {
                        "properties": {
                            "id": {
                                {% if synthetic_source | string | lower == "false" %}"ignore_above": 1024,{% endif %}
                                "type": "keyword"
                            },
                            "name": {
                                {% if synthetic_source | string | lower == "false" %}"ignore_above": 1024,{% endif %}
                                "type": "keyword"
                            }
                        }
                    },
                    "machine": {
                        "properties": {
                            "type": {
                                {% if synthetic_source | string | lower == "false" %}"ignore_above": 1024,{% endif %}
                                "type": "keyword"
                            }
                        }
                    },
                    "project": {
                        "properties": {
                            "id": {
                                {% if synthetic_source | string | lower == "false" %}"ignore_above": 1024,{% endif %}
                                "type": "keyword"
                            }
                        }
                    },
                    "provider": {
                        {% if synthetic_source | string | lower == "false" %}"ignore_above": 1024,{% endif %}
                        "type": "keyword"
                    },
                    "service": {
                        "properties": {
                            "name": {
                                {% if synthetic_source | string | lower == "false" %}"ignore_above": 1024,{% endif %}
                                "type": "keyword"
                            }
                        }
                    }
                }
            },
            {% if synthetic_source | string | lower == "false" %}
            "data_stream": {
                "properties": {
                    "dataset": {
                        "type": "constant_keyword",
                        "value": "elastic_agent.filebeat"
                    },
                    "namespace": {
                        "type": "constant_keyword",
                        "value": "default"
                    },
                    "type": {
                        "type": "constant_keyword",
                        "value": "logs"
                    }
                }
            },
            {% endif %}
            "ecs": {
                "properties": {
                    "version": {
                        {% if synthetic_source | string | lower == "false" %}"ignore_above": 1024,{% endif %}
                        "type": "keyword"
                    }
                }
            },
            "elastic_agent": {
                "properties": {
                    "id": {
                        {% if synthetic_source | string | lower == "false" %}"ignore_above": 1024,{% endif %}
                        "type": "keyword"
                    },
                    "snapshot": {
                        "type": "boolean"
                    },
                    "version": {
                        {% if synthetic_source | string | lower == "false" %}"ignore_above": 1024,{% endif %}
                        "type": "keyword"
                    }
                }
            },
            "event": {
                "properties": {
                    "dataset": {
                        {% if synthetic_source | string | lower == "false" %}"ignore_above": 1024,{% endif %}
                        "type": "keyword"
                    }
                }
            },
            "host": {
                "properties": {
                    "architecture": {
                        {% if synthetic_source | string | lower == "false" %}"ignore_above": 1024,{% endif %}
                        "type": "keyword"
                    },
                    "containerized": {
                        "type": "boolean"
                    },
                    "hostname": {
                        {% if synthetic_source | string | lower == "false" %}"ignore_above": 1024,{% endif %}
                        "type": "keyword"
                    },
                    "ip": {
                        "type": "ip"
                    },
                    "mac": {
                        {% if synthetic_source | string | lower == "false" %}"ignore_above": 1024,{% endif %}
                        "type": "keyword"
                    },
                    "name": {
                        {% if synthetic_source | string | lower == "false" %}"ignore_above": 1024,{% endif %}
                        "type": "keyword"
                    },
                    "os": {
                        "properties": {
                            "codename": {
                                {% if synthetic_source | string | lower == "false" %}"ignore_above": 1024,{% endif %}
                                "type": "keyword"
                            },
                            "family": {
                                {% if synthetic_source | string | lower == "false" %}"ignore_above": 1024,{% endif %}
                                "type": "keyword"
                            },
                            "kernel": {
                                {% if synthetic_source | string | lower == "false" %}"ignore_above": 1024,{% endif %}
                                "type": "keyword"
                            },
                            "name": {
                                {% if synthetic_source | string | lower == "false" %}"ignore_above": 1024,{% endif %}
                                "type": "keyword"
                            },
                            "platform": {
                                {% if synthetic_source | string | lower == "false" %}"ignore_above": 1024,{% endif %}
                                "type": "keyword"
                            },
                            "type": {
                                {% if synthetic_source | string | lower == "false" %}"ignore_above": 1024,{% endif %}
                                "type": "keyword"
                            },
                            "version": {
                                {% if synthetic_source | string | lower == "false" %}"ignore_above": 1024,{% endif %}
                                "type": "keyword"
                            }
                        }
                    }
                }
            },
            "id": {
                {% if synthetic_source | string | lower == "false" %}"ignore_above": 1024,{% endif %}
                "type": "keyword"
            },
            "input": {
                "properties": {
                    "type": {
                        {% if synthetic_source | string | lower == "false" %}"ignore_above": 1024,{% endif %}
                        "type": "keyword"
                    }
                }
            },
            "input_id": {
                {% if synthetic_source | string | lower == "false" %}"ignore_above": 1024,{% endif %}
                "type": "keyword"
            },
            "input_type": {
                {% if synthetic_source | string | lower == "false" %}"ignore_above": 1024,{% endif %}
                "type": "keyword"
            },
            "libbeat": {
                "properties": {
                    "processor": {
                        {% if synthetic_source | string | lower == "false" %}"ignore_above": 1024,{% endif %}
                        "type": "keyword"
                    }
                }
            },
            "log": {
                "properties": {
                    "file": {
                        "properties": {
                            "path": {
                                {% if synthetic_source | string | lower == "false" %}"ignore_above": 1024,{% endif %}
                                "type": "keyword"
                            }
                        }
                    },
                    "level": {
                        {% if synthetic_source | string | lower == "false" %}"ignore_above": 1024,{% endif %}
                        "type": "keyword"
                    },
                    "logger": {
                        {% if synthetic_source | string | lower == "false" %}"ignore_above": 1024,{% endif %}
                        "type": "keyword"
                    },
                    "offset": {
                        "type": "long"
                    },
                    "origin": {
                        "properties": {
                            "file": {
                                "properties": {
                                    "line": {
                                        "type": "long"
                                    },
                                    "name": {
                                        {% if synthetic_source | string | lower == "false" %}"ignore_above": 1024,{% endif %}
                                        "type": "keyword"
                                    }
                                }
                            }
                        }
                    }
                }
            },
            {% if synthetic_source | string | lower == "false" %}
            "message": {
                "type": "match_only_text"
            },
            {% endif %}

    "settings": {
        "index": {
            "codec": "best_compression",
            "hidden": "true",
            "lifecycle": {
                "name": "logs"
            },
            "number_of_replicas": "{{number_of_replicas | default(0)}}",
            "number_of_shards": "{{number_of_shards | default(1)}}",
            "query": {
                "default_field": [
                    "message"
                ]
            },
            "routing": {
                "allocation": {
                    "include": {
                        "_tier_preference": "data_hot"
                    }
                }
            }
        }
    }
}

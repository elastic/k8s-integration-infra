                                                "type": "long"
                                            },
                                            "events": {
                                                "properties": {
                                                    "active": {
                                                        "type": "long"
                                                    },
                                                    "published": {
                                                        "type": "long"
                                                    },
                                                    "retry": {
                                                        "type": "long"
                                                    },
                                                    "total": {
                                                        "type": "long"
                                                    }
                                                }
                                            },
                                            "queue": {
                                                "properties": {
                                                    "acked": {
                                                        "type": "long"
                                                    },
                                                    "max_events": {
                                                        "type": "long"
                                                    }
                                                }
                                            }
                                        }
                                    }
                                }
                            },
                            "registrar": {
                                "properties": {
                                    "states": {
                                        "properties": {
                                            "current": {
                                                "type": "long"
                                            }
                                        }
                                    }
                                }
                            },
                            "system": {
                                "properties": {
                                    "cpu": {
                                        "properties": {
                                            "cores": {
                                                "type": "long"
                                            }
                                        }
                                    },
                                    "handles": {

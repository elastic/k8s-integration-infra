                                                        "type": "long"
                                                    }
                                                }
                                            },
                                            "open": {
                                                "type": "long"
                                            }
                                        }
                                    },
                                    "load": {
                                        "properties": {
                                            "1": {
                                                "type": "float"
                                            },
                                            "15": {
                                                "type": "float"
                                            },
                                            "5": {
                                                "type": "float"
                                            },
                                            "norm": {
                                                "properties": {
                                                    "1": {
                                                        "type": "float"
                                                    },
                                                    "15": {
                                                        "type": "float"
                                                    },
                                                    "5": {
                                                        "type": "float"
                                                    }
                                                }
                                            }
                                        }
                                    }
                                }
                            }
